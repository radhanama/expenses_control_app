{
    "name": "Flutter Dev Container",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.flutter"
            ]
        }
    },
    "remoteEnv": {
        "CHROME_EXECUTABLE": "/usr/bin/chromium-browser"
    },
    "postCreateCommand": "flutter doctor"
}
